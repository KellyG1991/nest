{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "commit": "commit",
    "doc": "compodoc -p src/tsconfig.json",
    "doc:watch": "npm run doc -- -w",
    "doc:serve": "npm run doc -- -as",
    "doc:serve:watch": "npm run doc -- -s -w",
    "prebuild": "npm run gen:typings; rimraf dist",
    "gen:typings": "ts-node generate-typings.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "prestart": "npm i && npm run build",
    "start:dev": "nest start --watch",
    "start:development": "nest start --watch",
    "start:debug": "cross-env DEBUG=development:* nest start --debug --watch",
    "start:staging": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "start:production": "npm run start:prod",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "cross-env NODE_ENV_FILE_PATH='.env.test' node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "npm run gen:typings",
    "test:e2e": "cross-env NODE_ENV_FILE_PATH='.env.test' jest --config ./test/jest-e2e.json --forceExit"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@nestjs/common": "7.6.15",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.15",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/schedule": "0.4.3",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "compression": "1.7.4",
    "express-rate-limit": "5.2.6",
    "helmet": "4.4.1",
    "husky": "6.0.0",
    "moment-timezone": "0.5.28",
    "mongoose": "^5.12.3",
    "mongoose-autopopulate": "^0.12.3",
    "ms": "2.1.2",
    "nest-winston": "1.4.1",
    "rate-limit-redis": "2.1.0",
    "redis": "3.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.6",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.0",
    "@nestjs/testing": "7.6.15",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.36",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
